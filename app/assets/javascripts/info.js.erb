$(function() {
  $(".info").click(function(event){
    event.preventDefault();
    event.stopPropagation();
    $($(this).parent().parent().children()[4]).css('display', 'block')
  });


  $('.info-close').click(function(event){
    event.preventDefault();
    event.stopPropagation();
    $(".information").css("display", "none")
  });

